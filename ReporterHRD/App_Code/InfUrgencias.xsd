                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP (1000) ADNINGRESO.OID AS IdIngreso, ADNINGRESO.AINCONSEC AS NumIngreso, CASE WHEN GENPACIEN.PACTIPDOC = 1 THEN 'CC' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 2 THEN 'CE' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 3 THEN 'TI' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 4 THEN 'RC' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 5 THEN 'PA' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 6 THEN 'AS' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 7 THEN 'MS' ELSE CASE WHEN GENPACIEN.PACTIPDOC = 10 THEN 'CD' ELSE 'AS' END END END END END END END END AS TipoDocPac, GENPACIEN.PACNUMDOC AS NumDocPaciente, LTRIM(RTRIM(GENPACIEN.PACPRINOM + N' ' + GENPACIEN.PACSEGNOM)) AS NomPaciente, LTRIM(RTRIM(GENPACIEN.PACPRIAPE + N' ' + GENPACIEN.PACSEGAPE)) AS ApelPaciente, ADNINGRESO.AINFECING AS FechaIngreso, ADNINGRESO.AINURGCON AS TipoIngreso, HCNTRIAGE.HCTMOTCON AS MotivoConsulta, HCNTRIAGE.HCTALERGI AS Alergias, GENDETCON.GDENOMBRE AS EntidadPaciente, STR(DATEDIFF(month, GENPACIEN.GPAFECNAC, GETDATE()) / 12.0, 4, 1) AS EdadPac, HCNTRIAGE.HCTPESO AS PesoPac FROM ADNINGRESO INNER JOIN GENPACIEN ON ADNINGRESO.GENPACIEN = GENPACIEN.OID INNER JOIN GENDETCON ON ADNINGRESO.GENDETCON = GENDETCON.OID LEFT OUTER JOIN HCNTRIAGE ON ADNINGRESO.OID = HCNTRIAGE.ADNINGRESO WHERE (ADNINGRESO.OID = @IdIngreso) ORDER BY FechaIngreso DESC</CommandText>
